{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es6",
        "outDir": "assets/webview",
        "lib": ["es6", "dom"],
        "jsx": "react",
        "sourceMap": true,
        "rootDir": "./",
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true
    },
    "exclude": ["node_modules"]
}
